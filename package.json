{
  "name": "daterangepicker",
  "version": "3.1.1",
  "description": "Date range picker component",
  "main": "daterangepicker.js",
  "style": "daterangepicker.css",
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:js": "uglifyjs daterangepicker.js -o daterangepicker.min.js -c -m",
    "build:css": "cleancss -o daterangepicker.min.css daterangepicker.css"
  },
  "license": "MIT",
  "dependencies": {
    "jquery": ">=1.10",
    "moment": ">=2.9.0"
  },
  "devDependencies": {
    "clean-css-cli": "^5.6.2",
    "uglify-js": "^3.17.4"
  }
}